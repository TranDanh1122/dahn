{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "exclude": [
    "**/*.test.ts",
     "**/*.test.tsx",
    "**/__tests__/**"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@auth/*": [
        "src/modules/auth/*"
      ],
      "@lang/*": [
        "src/modules/language/*"
      ],
      "@workspace/*": [
        "src/modules/workspace/*"
      ],
      "@user/*": [
        "src/modules/user/*"
      ],
      "@project/*": [
        "src/modules/project/*"
      ],
      "@dashboard/*": [
        "src/modules/dashboard/*"
      ]
    }
  }
}