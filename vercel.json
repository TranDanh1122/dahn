{
  "version": 2,
  "builds": [
    {
      "src": "./FE/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "./BE/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "./BE/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "./FE/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "pnpm install && pnpm run build && mv FE/dist dist",
  "outputDirectory": "./FE/dist"
}